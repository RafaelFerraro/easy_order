<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Description</th>
    <th>Category</th>
  </tr>
  <% sub_categories.each do |sub_category| %>
  <tr>
    <td><%= sub_category.id %></td>
    <td><%= sub_category.name %></td>
    <td><%= sub_category.description %></td>
    <td><%= sub_category.category.name %></td>
  </tr>
  <% end %>
</table>

<%= link_to "New SubCategory", routes.new_sub_category_path %>