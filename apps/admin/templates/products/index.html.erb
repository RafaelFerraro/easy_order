<table>
  <tr>
    <th>Title</th>
    <th>Size</th>
    <th>Color</th>
    <th></th>
  </tr>
  <% products.each do |product| %>
    <tr>
      <td><%= product.title %></td>
      <td><%= product.size %></td>
      <td><%= product.color %></td>
      <td>Edit</td>
    </tr>
  <% end %>
</table>

<%= link_to 'New Product', routes.new_product_path %>